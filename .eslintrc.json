{
  "env": {
    "browser": false,
    "es6": true,
    "jest": true,
    "node": true
  },
  "extends": ["standard", "prettier", "prettier/standard"],
  "globals": {
    "Atomics": "readonly",
    "SharedArrayBuffer": "readonly"
  },
  "parser": "babel-eslint",
  "plugins": ["jest"],
  "rules": {
    "react/no-unused-prop-types": "off",
    "react/prop-types": "off",
    "react/jsx-no-bind": "off"
  }
}
